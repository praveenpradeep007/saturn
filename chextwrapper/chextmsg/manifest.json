{
	"manifest_version":2,
	"name":"ChextMsg",
	"version":"1",
	"content_scripts":[
		{
			"matches":[ "<all_urls>" ],
			"js":[ "js/jquery-3.2.1.min.js","js/scripts.js" ]
		}
	],
	"background": {
    	"scripts": ["js/background.js"]
  	},
  	"icons": { 
 		"128":"img/chextmsg.png"
 	},
  	"permissions": [
        "tabs",
        "notifications",
        "http://*/",
        "https://*/"
    ]
}